@page
@model WeatherModel

@{
    ViewData["Title"] = "Weather";
}

<table class="table">
    <tr>
        <th>Date</th>
        <th>Temp. C</th>
    </tr>
    @for (int i = 0; i < Model.WeatherData.hourly.time.Count; i++)
    {
        var time = Model.WeatherData.hourly.time[i];
        var temp = Model.WeatherData.hourly.temperature_2m[i];

        <tr>
            <td>@time</td>
            <td>@temp</td>
        </tr>
    }
</table>